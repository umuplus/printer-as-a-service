<xs:schema xmlns:eipjobcap="http://schemas.xerox.com/enterprise/eipjobcap/1" xmlns:eipjobmodel="http://schemas.xerox.com/enterprise/eipjobmodel/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://schemas.xerox.com/enterprise/eipjobcap/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.2.7">
	<xs:import namespace="http://schemas.xerox.com/enterprise/eipjobmodel/1" schemaLocation="JobModelSchemaCommon.xsd"/>
	<xs:element name="DeviceJobProcessingCapabilities">
		<!--Xerox Common Job Processing Capabilities-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MajorVersion" type="xs:short" default="1"/>
							<xs:element name="MinorVersion" type="xs:short" default="3"/>
							<xs:element name="Revision" type="xs:short" default="2"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="otherSchemaVersions" minOccurs="0">
					<xs:annotation>
						<xs:documentation>DeviceRestriction minOccurs=1</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="schema" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="name" type="xs:string">
											<xs:annotation>
												<xs:documentation>DeviceRestriction AllowedValues=JobModelSchemaCommon</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="version">
											<xs:complexType>
												<xs:all>
													<xs:element name="MajorVersion" type="xs:short"/>
													<xs:element name="MinorVersion" type="xs:short"/>
													<xs:element name="Revision" type="xs:short"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="eipjobcap:Input" minOccurs="0"/>
				<xs:element ref="eipjobcap:Output" minOccurs="0"/>
				<xs:element name="DefaultJobTickets" minOccurs="0">
					<!--Xerox Job Ticket with default values-->
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DefaultTicketPerService" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="CopyJobTicket" minOccurs="0">
											<!--Xerox Common Job Ticket-->
											<xs:complexType>
												<xs:sequence>
													<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
													<xs:element name="JobDescription" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="JobProcessing" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Input" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Scaling" minOccurs="0"/>
																			<xs:element name="Darkness" type="eipjobmodel:STDarkness" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Output" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="eipjobmodel:Finishings" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:ColorEffectsType" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:SheetCollate" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Copies" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:InputTraysCol" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="PrintJobTicket" minOccurs="0">
											<!--Xerox Common Job Ticket-->
											<xs:complexType>
												<xs:sequence>
													<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
													<xs:element name="JobDescription" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
																<xs:element ref="eipjobmodel:JobOriginatingUserName" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="JobProcessing" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Input" minOccurs="0">
																	<xs:complexType>
																		<xs:all>
																			<xs:element name="Accounting" minOccurs="0">
																				<xs:complexType>
																					<xs:all>
																						<xs:element name="Jba" minOccurs="0">
																							<xs:complexType>
																								<xs:all>
																									<xs:element ref="eipjobmodel:JobAccountingUserId" minOccurs="1"/>
																									<xs:element ref="eipjobmodel:JobAccountId" minOccurs="1"/>
																								</xs:all>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="Xsa" minOccurs="0">
																							<xs:complexType>
																								<xs:all>
																									<xs:element ref="eipjobmodel:JobAccountingType" minOccurs="0"/>
																									<xs:element ref="eipjobmodel:JobAccountingUserId" minOccurs="1"/>
																									<xs:element ref="eipjobmodel:JobAccountId" minOccurs="0"/>
																								</xs:all>
																							</xs:complexType>
																						</xs:element>
																					</xs:all>
																					<!--DeviceRestriction = choice-->
																				</xs:complexType>
																			</xs:element>
																		</xs:all>
																		<!--Accounting can only be xsa or jba but not both-->
																	</xs:complexType>
																</xs:element>
																<xs:element name="Output" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="eipjobmodel:Finishings" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:ColorEffectsType" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:SheetCollate" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Copies" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:InputTraysCol" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ScanJobTicket" minOccurs="0">
											<!--Xerox Common Job Ticket-->
											<xs:complexType>
												<xs:sequence>
													<xs:element name="schemaVersion" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="MajorVersion" type="xs:short" default="1"/>
																<xs:element name="MinorVersion" type="xs:short" default="2"/>
																<xs:element name="Revision" type="xs:short" default="3"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="JobDescription" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="JobProcessing" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Input" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="eipjobmodel:ColorMode" minOccurs="0"/>
																			<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
																			<xs:element name="OriginalType" type="eipjobmodel:CTOriginalType" minOccurs="0"/>
																			<xs:element name="ImageOptions" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Darkness" type="eipjobmodel:STDarkness" minOccurs="0"/>
																						<xs:element name="Sharpness" type="eipjobmodel:STSharpness" minOccurs="0"/>
																						<xs:element name="Contrast" type="eipjobmodel:CTContrast" minOccurs="0"/>
																						<xs:element name="AutoExposure" type="eipjobmodel:STAutoExposure" minOccurs="0"/>
																						<xs:element name="Resolution" type="eipjobmodel:STResolutionType" minOccurs="0"/>
																						<xs:element name="Saturation" type="eipjobmodel:STSaturation" minOccurs="0"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="LayoutAdjustment" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="InputOrientation" type="eipjobmodel:STInputOrientationType" minOccurs="0"/>
																						<xs:element name="InputMediaSize" type="eipjobmodel:CTInputMediaSize" minOccurs="0"/>
																						<xs:element name="InputEdgeErase" type="eipjobmodel:CTEdgeErase" minOccurs="0"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="ImageSettings" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Compression" type="eipjobmodel:STImageCompressionType" minOccurs="0"/>
																						<xs:element name="BitsPerPixel" type="eipjobmodel:STBitsPerPixelType" minOccurs="0"/>
																						<xs:element name="OptimizedForFastWebView" type="xs:boolean" minOccurs="0"/>
																						<xs:element name="SearchableText" type="eipjobmodel:STSearchableText" minOccurs="0"/>
																						<xs:element name="SourceDocumentLanguages" type="eipjobmodel:STSourceDocumentLanguages" minOccurs="0"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Output" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Destination" type="eipjobmodel:CTDestination" minOccurs="0"/>
																			<xs:element name="MetaData" type="eipjobmodel:CTMetaData" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DeviceJobProcessingCapabilitiesByServices" minOccurs="0">
					<!--Xerox Common Job Processing Capabilities-->
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CapabilitiesByService" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="ServiceType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<!--		These values are listed for reference only. Do not uncomment.
							Un-commenting these values will break compatibility when new values are added.-->
													<!---
					<xs:enumeration value="Copy"/>
					<xs:enumeration value="Print"/>
					<xs:enumeration value="WorkflowScanning"/>
					<xs:enumeration value="IncomingFax"/>
					<xs:enumeration value="FaxSend"/>
					<xs:enumeration value="Email"/>
					<xs:enumeration value="InternetFaxReceive"/>
					<xs:enumeration value="InternetFaxSend"/>
					<xs:enumeration value="ReportJob"/>
					<xs:enumeration value="FaxSendFromPc"/>
					-->
												</xs:restriction>
												<!--This enum is identical to the Job Model (TODO:  include job model schema here?)-->
											</xs:simpleType>
										</xs:element>
										<xs:element name="DeviceJobProcessingCapabilities">
											<!--Xerox Common Job Processing Capabilities-->
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="eipjobcap:Input" minOccurs="0"/>
													<xs:element ref="eipjobcap:Output" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InputTrayEntry">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InputTrayName" type="eipjobmodel:STInputTrayName" minOccurs="0"/>
				<xs:element name="TrayLevel" type="eipjobmodel:STTrayLevel" minOccurs="0"/>
				<xs:element name="MediaCollectionCurrent" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaSize" type="eipjobmodel:CTMediaSize" minOccurs="0"/>
							<xs:element name="MediaColor" type="eipjobmodel:STMediaColor" minOccurs="0"/>
							<xs:element name="MediaType" type="eipjobmodel:STMediaType" minOccurs="0"/>
							<xs:element name="FeedOrientation" type="eipjobmodel:STFeedOrientation" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="MediaCollectionSupported" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Note: Up to 2 MediaCollectionSupported elements are supported in the InputTrayEntrySupported element. This would happen when the devise wants to distinguish between the media sizes that it can feed long edge first vs. the media size it can feed short edge first. In this case one MediaCollectionSupported element would contain a FeedOrientationSupported element with a single value of LongEdgeFirst and the other MediaCollectionSupported element would contain a FeedOrientationSupported element with a single value of ShortEdgeFirst.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaSizeSupported" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="XDimensionSupported">
											<xs:annotation>
												<xs:documentation>The value is specified in hundredths of millimeters. XDimension is the short edge of the media.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:annotation>
														<xs:documentation>The value is specified in hundredths of millimeters. The minimum value for x will always be less than or equal to the minimum value for y.</xs:documentation>
													</xs:annotation>
													<xs:element name="MinimumValue">
														<xs:simpleType>
															<xs:restriction base="xs:unsignedInt">
																<xs:minInclusive value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="MaximumValue" type="xs:unsignedInt">
														<xs:annotation>
															<xs:documentation>The value is specified in hundredths of millimeters. The maximum value for x will always be less than or equal to the maximum value for y.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="YDimensionSupported">
											<xs:annotation>
												<xs:documentation>The value is specified in hundredths of millimeters. YDimension is the long edge of the media.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:annotation>
														<xs:documentation>The value is specified in hundredths of millimeters. The minimum value for x will always be less than or equal to the minimum value for y.</xs:documentation>
													</xs:annotation>
													<xs:element name="MinimumValue">
														<xs:simpleType>
															<xs:restriction base="xs:unsignedInt">
																<xs:minInclusive value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="MaximumValue" type="xs:unsignedInt">
														<xs:annotation>
															<xs:documentation>The value is specified in hundredths of millimeters. The maximum value for x will always be less than or equal to the maximum value for y.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
								<!--<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobcap:MediaSizeSupported" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>-->
							</xs:element>
							<xs:element name="MediaColorSupported" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MediaColorKeywordSupported" type="eipjobmodel:STMediaColor" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="MediaColorCustomSupported" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Note: If this element is not included then custom colors are not supported in the tray.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="MediaTypeSupported" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MediaTypeKeywordSupported" type="eipjobmodel:STMediaType" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="MediaTypeCustomSupported" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Note: If this element is not included then custom types are not supported in the tray.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="FeedOrientationSupported" type="eipjobmodel:STFeedOrientation" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Input">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SidesCapabilities" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SidesDefault" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="DefaultValue" type="eipjobmodel:STSides"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SidesSupported" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AllowedValue" type="eipjobmodel:STSides" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ScalingCapabilities" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ScalingDefault" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ScalingTypeDefault" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="DefaultValue" type="eipjobmodel:STScalingType"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ScaleProportionalDefault" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="DefaultValue" type="eipjobmodel:STScaling"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ScaleIndependentDefault" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="XScaling">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="DefaultValue" type="eipjobmodel:STScaling"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="YScaling">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="DefaultValue" type="eipjobmodel:STScaling"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ScalingSupported" minOccurs="0">
								<xs:complexType>
