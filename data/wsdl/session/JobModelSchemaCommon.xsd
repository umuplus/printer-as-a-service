<xs:schema xmlns:eipjobmodel="http://schemas.xerox.com/enterprise/eipjobmodel/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://schemas.xerox.com/enterprise/eipjobmodel/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.5.17">
	<!--If enum value is not recognized or left out, the device default will be used.  -->
	<!--The Job Queue-->
	<xs:complexType name="schemaVersionType">
		<xs:sequence>
			<xs:element name="MajorVersion" type="xs:short" default="1"/>
			<xs:element name="MinorVersion" type="xs:short" default="5"/>
			<xs:element name="Revision" type="xs:short" default="17"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="JobQueue">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element ref="eipjobmodel:JobInfo" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--The Copy Job (used for getting details)-->
	<xs:element name="CopyJob">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element ref="eipjobmodel:JobInfo"/>
				<xs:element ref="eipjobmodel:CopyJobTicket" minOccurs="0"/>
				<xs:element ref="eipjobmodel:CopyJobReceipt" minOccurs="0"/>
			</xs:sequence>
			<!--Job - object -->
		</xs:complexType>
	</xs:element>
	<xs:element name="ScanJob">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element ref="eipjobmodel:JobInfo"/>
				<xs:element ref="eipjobmodel:ScanJobTicket" minOccurs="0"/>
				<xs:element ref="eipjobmodel:ScanJobReceipt" minOccurs="0"/>
			</xs:sequence>
			<!--Job - object -->
		</xs:complexType>
	</xs:element>
	<!--Parts of a job-->
	<xs:element name="JobInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element ref="eipjobmodel:JobId"/>
				<xs:element ref="eipjobmodel:ClientId" minOccurs="0"/>
				<xs:element ref="eipjobmodel:JobName"/>
				<xs:element ref="eipjobmodel:JobType"/>
				<xs:element ref="eipjobmodel:JobState"/>
				<xs:element ref="eipjobmodel:JobOriginatingUserName"/>
				<xs:element ref="eipjobmodel:JobStateReasons" maxOccurs="unbounded"/>
				<xs:element ref="eipjobmodel:DateTimeAtCompleted" minOccurs="0"/>
				<xs:element ref="eipjobmodel:DateTimeAtCreation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrintJobTicket">
		<!--Xerox Common Job Ticket-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element name="JobDescription" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
							<xs:element ref="eipjobmodel:JobOriginatingUserName" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JobProcessing" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Input" minOccurs="0">
								<xs:complexType>
									<xs:all>
										<xs:element name="Accounting" minOccurs="0">
											<xs:complexType>
												<xs:all>
													<xs:element name="Jba" minOccurs="0">
														<xs:complexType>
															<xs:all>
																<xs:element ref="eipjobmodel:JobAccountingUserId"/>
																<xs:element ref="eipjobmodel:JobAccountId"/>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="Xsa" type="eipjobmodel:CTJobAccountingInfo" minOccurs="0"/>
												</xs:all>
												<!--DeviceRestriction = choice-->
											</xs:complexType>
										</xs:element>
									</xs:all>
									<!--Accounting can only be xsa or jba but not both-->
								</xs:complexType>
							</xs:element>
							<xs:element name="Output" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:Finishings" minOccurs="0"/>
										<xs:element ref="eipjobmodel:ColorEffectsType" minOccurs="0"/>
										<xs:element ref="eipjobmodel:SheetCollate" minOccurs="0"/>
										<xs:element ref="eipjobmodel:Copies" minOccurs="0"/>
										<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
										<xs:choice minOccurs="0">
											<xs:element ref="eipjobmodel:InputTraysCol"/>
											<xs:element ref="eipjobmodel:MediaCol"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CopyJobTicket">
		<!--Xerox Common Job Ticket-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element name="JobDescription" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JobProcessing" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Input" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
										<xs:element ref="eipjobmodel:Scaling" minOccurs="0"/>
										<xs:element name="Darkness" type="eipjobmodel:STDarkness" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Output" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:Finishings" minOccurs="0"/>
										<xs:element ref="eipjobmodel:ColorEffectsType" minOccurs="0"/>
										<xs:element ref="eipjobmodel:SheetCollate" minOccurs="0"/>
										<xs:element ref="eipjobmodel:Copies" minOccurs="0"/>
										<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
										<xs:choice minOccurs="0">
											<xs:element ref="eipjobmodel:InputTraysCol"/>
											<xs:element ref="eipjobmodel:MediaCol"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CopyJobReceipt">
		<!--Xerox Common Job Ticket-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="JobDescription" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JobProcessing">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Input">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:Sides"/>
										<xs:element ref="eipjobmodel:Scaling"/>
										<xs:element name="Darkness" type="eipjobmodel:STDarkness" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Output">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:Finishings"/>
										<xs:element ref="eipjobmodel:ColorEffectsType"/>
										<xs:element ref="eipjobmodel:SheetCollate"/>
										<xs:element ref="eipjobmodel:Copies"/>
										<xs:element ref="eipjobmodel:Sides"/>
										<xs:element ref="eipjobmodel:InputTraysCol"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ScanJobTicket">
		<!--Xerox Common Job Ticket-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="schemaVersion" type="eipjobmodel:schemaVersionType" minOccurs="0"/>
				<xs:element name="JobDescription" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JobProcessing" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Input" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:ColorMode" minOccurs="0">
											<!--DeviceRestriction=mustUnderstand -->
										</xs:element>
										<xs:element ref="eipjobmodel:Sides" minOccurs="0"/>
										<xs:element name="OriginalType" type="eipjobmodel:CTOriginalType" minOccurs="0"/>
										<xs:element name="ImageOptions" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Darkness" type="eipjobmodel:STDarkness" minOccurs="0"/>
													<xs:element name="Sharpness" type="eipjobmodel:STSharpness" minOccurs="0"/>
													<xs:element name="Contrast" type="eipjobmodel:CTContrast" minOccurs="0"/>
													<xs:element name="AutoExposure" type="eipjobmodel:STAutoExposure" minOccurs="0"/>
													<xs:element name="Resolution" type="eipjobmodel:STResolutionType" minOccurs="0">
														<!--DeviceRestriction=mustUnderstand -->
													</xs:element>
													<xs:element name="Saturation" type="eipjobmodel:STSaturation" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="LayoutAdjustment" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="InputOrientation" type="eipjobmodel:STInputOrientationType" minOccurs="0"/>
													<xs:element name="InputMediaSize" type="eipjobmodel:CTInputMediaSize" minOccurs="0"/>
													<xs:element name="InputEdgeErase" type="eipjobmodel:CTEdgeErase" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ImageSettings" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="BitsPerPixel" type="eipjobmodel:STBitsPerPixelType" minOccurs="0"/>
													<xs:element name="OptimizedForFastWebView" type="xs:boolean" minOccurs="0"/>
													<xs:element name="SearchableText" type="eipjobmodel:STSearchableText" minOccurs="0"/>
													<xs:element name="SourceDocumentLanguages" type="eipjobmodel:STSourceDocumentLanguages" minOccurs="0"/>
													<xs:element name="CompressionQualitySetting" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="CompressionQuality" type="eipjobmodel:STCompressionQuality" minOccurs="0">
																	<!--Default value is HighCompression-->
																</xs:element>
																<xs:element name="CompressionCompatability" type="eipjobmodel:STCompressionCompatability" minOccurs="0">
																	<!--Default value is HighCompatibility-->
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Accounting" minOccurs="0">
											<xs:complexType>
												<xs:all>
													<xs:element name="Jba" minOccurs="0">
														<xs:complexType>
															<xs:all>
																<xs:element ref="eipjobmodel:JobAccountingUserId"/>
																<xs:element ref="eipjobmodel:JobAccountId"/>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="Xsa" type="eipjobmodel:CTJobAccountingInfo" minOccurs="0"/>
												</xs:all>
												<!--DeviceRestriction = choice-->
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Output" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Destination" type="eipjobmodel:CTDestination" minOccurs="0"/>
										<xs:element name="MetaData" type="eipjobmodel:CTMetaData" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ScanJobReceipt">
		<!--Xerox Common Job Ticket-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="JobDescription" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="eipjobmodel:JobName" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JobProcessing" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Input">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="eipjobmodel:ColorMode"/>
										<xs:element ref="eipjobmodel:Sides"/>
										<xs:element name="OriginalType" type="eipjobmodel:CTOriginalType"/>
										<xs:element name="ImageOptions">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Darkness" type="eipjobmodel:STDarkness"/>
													<xs:element name="Sharpness" type="eipjobmodel:STSharpness"/>
													<xs:element name="Contrast" type="eipjobmodel:CTContrast"/>
													<xs:element name="AutoExposure" type="eipjobmodel:STAutoExposure"/>
													<xs:element name="Resolution" type="eipjobmodel:STResolutionType"/>
													<xs:element name="Saturation" type="eipjobmodel:STSaturation"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="LayoutAdjustment">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="InputOrientation" type="eipjobmodel:STInputOrientationType"/>
													<xs:element name="InputMediaSize" type="eipjobmodel:CTInputMediaSize"/>
													<xs:element name="InputEdgeErase" type="eipjobmodel:CTEdgeErase"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ImageSettings" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="BitsPerPixel" type="eipjobmodel:STBitsPerPixelType"/>
													<xs:element name="OptimizedForFastWebView" type="xs:boolean"/>
													<xs:element name="SearchableText" type="eipjobmodel:STSearchableText"/>
													<xs:element name="SourceDocumentLanguages" type="eipjobmodel:STSourceDocumentLanguages"/>
													<xs:element name="CompressionQualitySetting" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="CompressionQuality" type="eipjobmodel:STCompressionQuality" minOccurs="0">
																	<!--Default value is MediumCompression-->
																</xs:element>
																<xs:element name="CompressionCompatability" type="eipjobmodel:STCompressionCompatability" minOccurs="0">
