<div class="col-md-3 col-sm-6 mt-3">
    <div class="card text-white bg-dark">
        <div class="card-header">
            <h4 class="my-0">
                <a href="/users?ts=<%- $qs.val('ts') %>" class="text-decoration-none text-white">
                    <i class="fa fa-users float-right"></i>
                    <%= __('app.users') %>
                </a>
            </h4>
        </div>
        <div class="card-body">
            <h2 class="card-title pricing-card-title">
                <%- usersActive %> <%- __('field.active') %>
                <small>/ <%- usersActive + usersPassive %> <%- __('field.total') %></small>
            </h2>
        </div>
    </div>
</div>
